{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "8.10"
        }
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-export-extensions",
    "transform-object-rest-spread",
    [
      "transform-runtime",
      {
        "polyfill": false
      }
    ]
  ],
  "ignore": [
    "node_modules"
  ]
}
