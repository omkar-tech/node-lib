{
  "presets": [
    "es2017",
    "es2016",
    "es2015-node6"
  ],
  "plugins": [
    "add-module-exports",
    "transform-class-properties",
    "transform-export-extensions",
    "transform-object-rest-spread",
    [ "transform-runtime", { "polyfill": false } ]
  ],
  "ignore": [
    "node_modules"
  ]
}